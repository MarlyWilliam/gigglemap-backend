generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Place {
  id          Int                                   @id @default(autoincrement())
  name        String
  description String?
  location    Unsupported("geography(Point, 4326)")
}
